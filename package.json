{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "generate-seo": "node scripts/generateStaticSeoFiles.js",
    "dev": "npm run generate-seo && concurrently \"vite\" \"node server/index.js\"",
    "build": "npm run generate-seo && node --max-old-space-size=8192 ./node_modules/vite/bin/vite.js build",
    "build:analyze": "npm run generate-seo && vite build --mode analyze",
    "start": "node server/index.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:quiz": "vitest --config vitest.integration.config.ts src/test/integration/quizGameIntegration.test.ts",
    "test:quiz-watch": "vitest --config vitest.integration.config.ts src/test/integration/quizGameIntegration.test.ts --watch",
    "test:quiz-answers": "vitest --config vitest.integration.config.ts src/test/integration/playerAnswerTesting.test.ts",
    "test:quiz-all": "vitest --config vitest.integration.config.ts src/test/integration/*.test.ts",
    "test:deposit-prize": "vitest --config vitest.integration.config.ts src/test/integration/depositPrizeAsset.test.ts",
    "fetch-idl": "ts-node scripts/fetchIdl.ts",
    "sync-idl": "ts-node scripts/sync-idl.ts",
    "verify-idl": "ts-node scripts/verify-idl.ts"
  },
