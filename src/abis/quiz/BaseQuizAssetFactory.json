[
  {
    "type": "constructor",
    "inputs": [
      { "name": "platformWallet", "type": "address", "internalType": "address" }
    ],
    "stateMutability": "nonpayable"
  },

  { "type": "function", "name": "PLATFORM_WALLET", "inputs": [], "outputs": [{ "type": "address" }], "stateMutability": "view" },
  { "type": "function", "name": "owner", "inputs": [], "outputs": [{ "type": "address" }], "stateMutability": "view" },

  {
    "type": "function",
    "name": "allowedToken",
    "inputs": [ { "name": "", "type": "address" } ],
    "outputs": [ { "type": "bool" } ],
    "stateMutability": "view"
  },

  {
    "type": "function",
    "name": "roomKeyOf",
    "inputs": [ { "name": "roomId", "type": "string" } ],
    "outputs": [ { "type": "bytes32" } ],
    "stateMutability": "pure"
  },

  {
    "type": "function",
    "name": "roomOf",
    "inputs": [ { "name": "", "type": "bytes32" } ],
    "outputs": [ { "type": "address" } ],
    "stateMutability": "view"
  },

  {
    "type": "function",
    "name": "setAllowedToken",
    "inputs": [
      { "name": "token", "type": "address" },
      { "name": "allowed", "type": "bool" }
    ],
    "outputs": [],
    "stateMutability": "nonpayable"
  },

  {
    "type": "function",
    "name": "createAssetRoom",
    "inputs": [
      { "name": "roomId", "type": "string" },
      { "name": "paymentToken", "type": "address" },
      { "name": "host", "type": "address" },
      { "name": "entryFee", "type": "uint256" },
      { "name": "hostPayBps", "type": "uint16" }
    ],
    "outputs": [
      { "name": "room", "type": "address" }
    ],
    "stateMutability": "nonpayable"
  },

  {
    "type": "function",
    "name": "getRoomByRoomId",
    "inputs": [ { "name": "roomId", "type": "string" } ],
    "outputs": [ { "type": "address" } ],
    "stateMutability": "view"
  },

  {
    "type": "event",
    "name": "TokenAllowed",
    "anonymous": false,
    "inputs": [
      { "name": "token", "type": "address", "indexed": false },
      { "name": "allowed", "type": "bool", "indexed": false }
    ]
  },

  {
    "type": "event",
    "name": "RoomCreated",
    "anonymous": false,
    "inputs": [
      { "name": "roomKey", "type": "bytes32", "indexed": true },
      { "name": "roomId", "type": "string", "indexed": false },
      { "name": "room", "type": "address", "indexed": false },
      { "name": "paymentToken", "type": "address", "indexed": false },
      { "name": "host", "type": "address", "indexed": false },
      { "name": "entryFee", "type": "uint256", "indexed": false }
    ]
  },

  {
    "type": "event",
    "name": "RoomClosedByAdmin",
    "inputs": [
      { "name": "room", "type": "address", "indexed": false }
    ],
    "anonymous": false
  },

  {
    "type": "event",
    "name": "OwnershipTransferred",
    "inputs": [
      { "name": "previousOwner", "type": "address", "indexed": true },
      { "name": "newOwner", "type": "address", "indexed": true }
    ],
    "anonymous": false
  }
]

